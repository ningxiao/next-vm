{
    "name": "next-vm",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev -p 8080",
        "start": "next start -p 8080",
        "lint": "next lint",
        "build": "next build",
        "prettier": "prettier --write src/ --ext .ts,.tsx,.js,.jsx",
        "pre-commit": "npm run prettier && npm run lint",
        "prepare": "husky install && husky add .husky/pre-commit 'npm run pre-commit'"
    },
    "dependencies": {
        "swr": "^1.3.0",
        "vm2": "^3.9.11",
        "next": "12.3.1",
        "react": "18.2.0",
        "sqlite3": "^5.1.2",
        "react-dom": "18.2.0",
        "@arco-design/web-react": "^2.40.2"
    },
    "devDependencies": {
        "@types/node": "18.11.3",
        "@types/react": "18.0.21",
        "@types/react-dom": "18.0.6",
        "husky": "^7.0.2",
        "eslint": "8.25.0",
        "prettier": "^2.4.1",
        "postcss": "^8.4.14",
        "typescript": "4.8.4",
        "tailwindcss": "^3.1.6",
        "autoprefixer": "^10.4.7",
        "eslint-config-next": "12.3.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0"
    }
}
